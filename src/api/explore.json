---
layout: none
permalink: api/explore
---
[
  {% assign pages = site.pages | where: 'layout', 'page' %}
  {% for page in pages %}
    {
      "layout": {{ page['layout'] | jsonify }},
      "url": {{ page['url'] | jsonify }},
      "title": {{ page['title'] | jsonify }},
      "content": {{ page['content'] | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]